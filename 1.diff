+#define MLOG_TAG "MediaAlbumsControllerServiceTest"
+
+#include "album_get_assets_test.h"
+
+#include <string>
+#include <vector>
+
+#define private public
+#define protected public
+#include "media_albums_controller_service.h"
+#undef private
+#undef protected
+
+#include "album_get_assets_vo.h"
